{
  "name": "wpfeatureloop-sdk",
  "version": "1.1.3",
  "description": "Feature voting widget SDK for WordPress plugins",
  "main": "dist/wpfeatureloop.umd.js",
  "module": "dist/wpfeatureloop.es.js",
  "types": "dist/wpfeatureloop.d.ts",
  "style": "dist/wpfeatureloop.css",
  "files": [
    "dist"
  ],
  "scripts": {
    "dev": "vite",
    "build": "vite build && npm run build:css",
    "build:css": "lightningcss --minify --bundle src/wpfeatureloop.css -o dist/wpfeatureloop.min.css && cp src/wpfeatureloop.css dist/wpfeatureloop.css",
    "test": "vitest",
    "test:run": "vitest run",
    "test:coverage": "vitest run --coverage",
    "preview": "vite preview"
  },
  "keywords": [
    "wpfeatureloop",
    "feature-loop",
    "voting",
    "feedback",
    "roadmap",
    "wordpress",
    "widget",
    "sdk"
  ],
  "author": "",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/eduardovillao/wpfeatureloop-js-sdk/issues"
  },
  "homepage": "https://wpfeatureloop.com/",
  "devDependencies": {
    "happy-dom": "^13.3.8",
    "lightningcss-cli": "^1.23.0",
    "vite": "^5.0.12",
    "vitest": "^1.2.2"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/eduardovillao/wpfeatureloop-js-sdk"
  }
}
